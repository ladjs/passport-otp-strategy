{
  "name": "@ladjs/passport-otp-strategy",
  "description": "TOTP authentication strategy for Passport.",
  "version": "1.1.0",
  "author": "Eric Hayes <eric@deployfx.com> (https://github.com/ejhayes)",
  "bugs": {
    "url": "http://github.com/ladjs/passport-otp-strategy/issues"
  },
  "contributors": [
    "Eric Hayes <eric@deployfx.com> (https://github.com/ejhayes)",
    "Jared Hanson (https://github.com/jaredhanson)"
  ],
  "dependencies": {
    "otplib": "^12.0.1",
    "passport-strategy": "^1.0.0",
    "pkginfo": "^0.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "ava": "^4.2.0",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "eslint": "8.16.0",
    "eslint-config-xo-lass": "1.0.6",
    "fixpack": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "remark-cli": "10.0.1",
    "remark-preset-github": "4.0.1",
    "xo": "^0.49.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/ladjs/passport-otp-strategy",
  "keywords": [
    "auth",
    "authentication",
    "authn",
    "oath",
    "otp",
    "passport",
    "totp"
  ],
  "license": "MIT",
  "main": "./lib",
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ladjs/passport-otp-strategy.git"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
